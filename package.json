{
  "name": "nextjs-boilerplate",
  "description": "nextjs-boilerplate",
  "version": "0.1.0",
  "repository": "https://github.com/tjnk24/nextjs-boilerplate.git",
  "author": "tjnk24 <tjnk24@gmail.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "dev": "dotenv npm run open-browser && bnr start-dev",
    "build": "next build",
    "start": "next start",
    "lint": "npm-run-all lint:*",
    "lint:tsc": "npx tsc -p tsconfig.json --skipLibCheck",
    "lint:src": "npx eslint --ext=.ts,.tsx ./",
    "lint:style": "npx stylelint **/*.less",
    "lint-fix": "eslint ./src/**/*.ts --fix & eslint ./src/**/*.tsx --fix & stylelint ./src/**/*.less --fix",
    "postinstall": "npx husky install",
    "test": "npm-run-all lint",
    "open-browser": "start http://localhost:%PORT%/users"
  },
  "dependencies": {
    "@ant-design/icons": "5.2.6",
    "@reduxjs/toolkit": "2.0.1",
    "antd": "5.13.0",
    "axios": "1.6.5",
    "next": "13.5.6",
    "next-redux-wrapper": "8.1.0",
    "query-string": "8.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-redux": "9.1.0",
    "redux-logger": "3.0.6",
    "reselect": "5.1.0",
    "sharp": "0.33.2"
  },
  "devDependencies": {
    "@types/history": "5.0.0",
    "@types/lodash": "4.14.202",
    "@types/node": "18.11.18",
    "@types/react": "18.2.47",
    "@types/react-dom": "18.2.18",
    "@types/redux-logger": "3.0.12",
    "@typescript-eslint/eslint-plugin": "6.18.1",
    "better-npm-run": "0.1.1",
    "dotenv-cli": "7.3.0",
    "eslint": "8.56.0",
    "eslint-config-next": "13.5.6",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-eslint-plugin": "5.2.1",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jsx-a11y": "6.8.0",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-webpack-plugin": "4.0.1",
    "fork-ts-checker-webpack-plugin": "9.0.2",
    "husky": "8.0.3",
    "lodash": "4.17.21",
    "lodash-webpack-plugin": "0.11.6",
    "next-with-less": "3.0.1",
    "npm-run-all": "4.1.5",
    "postcss-less": "6.0.0",
    "stylelint": "16.1.0",
    "stylelint-config-standard": "36.0.0",
    "typescript": "5.3.3"
  },
  "betterScripts": {
    "start-dev": {
      "command": "next dev -p %PORT%",
      "env": {
        "NODE_OPTIONS": "--inspect",
        "NODE_ENV": "development"
      }
    }
  }
}
