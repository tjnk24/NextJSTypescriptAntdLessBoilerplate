{
  "name": "nextjs-typescript-antd-less-boilerplate",
  "description": "nextjs-typescript-antd-less-boilerplate",
  "version": "0.1.0",
  "repository": "https://github.com/tjnk24/NextJsTypescriptAntdLessBoilerplate.git",
  "author": "tjnk24 <tjnk24@gmail.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "dev": "npm run open-browser && bnr start-dev",
    "build": "next build",
    "start": "next start",
    "lint": "npm-run-all lint:*",
    "lint:tsc": "npx tsc -p tsconfig.json --skipLibCheck",
    "lint:src": "npx eslint --ext=.ts,.tsx ./",
    "lint:style": "npx stylelint **/*.less",
    "lint-fix": "eslint ./src/**/*.ts --fix & eslint ./src/**/*.tsx --fix & stylelint ./src/**/*.less --fix",
    "postinstall": "npx husky install",
    "test": "npm-run-all lint",
    "open-browser": "start http://localhost:3000/users"
  },
  "dependencies": {
    "@ant-design/icons": "5.0.1",
    "@reduxjs/toolkit": "1.9.3",
    "antd": "5.2.3",
    "axios": "1.3.4",
    "next": "13.2.4",
    "next-redux-wrapper": "8.1.0",
    "query-string": "8.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-redux": "8.0.5",
    "redux-logger": "3.0.6",
    "reselect": "4.1.7",
    "sharp": "0.32.0"
  },
  "devDependencies": {
    "@types/history": "4.7.11",
    "@types/lodash": "4.14.191",
    "@types/node": "18.11.18",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.10",
    "@types/redux-logger": "3.0.9",
    "@typescript-eslint/eslint-plugin": "5.57.1",
    "@typescript-eslint/parser": "5.57.1",
    "better-npm-run": "0.1.1",
    "eslint": "8.37.0",
    "eslint-config-next": "13.3.0",
    "eslint-config-react-app": "7.0.1",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-eslint-plugin": "5.0.8",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-webpack-plugin": "4.0.0",
    "fork-ts-checker-webpack-plugin": "8.0.0",
    "husky": "8.0.3",
    "lodash": "4.17.21",
    "lodash-webpack-plugin": "0.11.6",
    "next-with-less": "2.0.5",
    "npm-run-all": "4.1.5",
    "postcss-less": "6.0.0",
    "stylelint": "15.4.0",
    "stylelint-config-standard": "32.0.0",
    "typescript": "4.9.4",
    "typescript-plugin-css-modules": "4.2.3"
  },
  "betterScripts": {
    "start-dev": {
      "command": "next dev",
      "env": {
        "NODE_OPTIONS": "--inspect"
      }
    }
  }
}
